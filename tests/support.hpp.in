#include "gtest/gtest.h"

namespace tests {
static std::string fixture(const std::string & filename){
    std::string name ="@CMAKE_RUNTIME_OUTPUT_DIRECTORY@/" + filename;
    return name;
}

static std::string fixture_files(const std::string & filename){
    std::string name ="@CMAKE_SOURCE_DIR@/tests/fixtures/" + filename;
    return name;
}
}